{
  "name": "Flat-Compiler",
  "version": "0.1.0",
  "description": "Flat Compiler",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js"
  },
  "sources": {
    "main": {
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Flat-Lexer": "master",
        "https://github.com/FlatLang/Parser-Core": "master",
        "https://github.com/FlatLang/Flat-Parser": "master",
        "https://github.com/FlatLang/Java-Parser": "master",
        "https://github.com/FlatLang/Flat-Compiler-Models": "master",
        "https://github.com/FlatLang/Flat-Flat-Writer": "master",
        "https://github.com/FlatLang/Flat-ES6-Writer": "master",
        "https://github.com/FlatLang/Time": "master",
        "https://github.com/FlatLang/Logger": "master",
        "https://github.com/FlatLang/FuCli": "master",
        "https://github.com/FlatLang/Regex": "master",
        "https://github.com/FlatLang/Progress-Bar": "master",
        "https://github.com/FlatLang/Console-Escape-Codes": "master",
        "https://github.com/FlatLang/TerminalSize": "master"
      }
    },
    "flatc": {
      "mainClass": "flat/compiler/Compiler",
      "bin": "flatc",
      "dependencies": {}
    },
    "test": {
      "mainClass": "flat/compiler/Compiler_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    }
  }
}