{
  "name": "Flat-Compiler",
  "version": "0.1.0",
  "description": "Flat Compiler",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/test.js"
  },
  "sources": {
    "main": {
      "source": "src",
      "dependencies": {
        "github:FlatLang/System": "master",
        "github:FlatLang/IO": "master",
        "github:FlatLang/Flat-Lexer": "master",
        "github:FlatLang/Flat-Parser": "master",
        "github:FlatLang/Flat-Compiler-Models": "master",
        "github:FlatLang/Time": "master",
        "github:FlatLang/Logger": "master"
      }
    },
    "flatc": {
      "mainClass": "flat/compiler/Compiler",
      "bin": "flatc",
      "dependencies": {}
    },
    "test": {
      "mainClass": "flat/compiler/Compiler_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "github:FlatLang/Nest": "master"
      }
    }
  }
}